{
  "name": "Mollydobbin tracking tool",
  "version": "0.0.1",
  "description": "Tool to show tracking information on http://next.ft.com/.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_title": "Mollydobbin",
    "default_icon": "icon.png",
    "default_popup": "popup/popup.html"
  },
  "content_scripts":[{
    "js":["jquery-2.1.3.min.js","content_script.js"],
    "matches":["http://*.next.ft.com/*","https://*.next.ft.com/*"]
  }],
  "permissions": [
    "declarativeContent"
  ],
  "manifest_version": 2
}
